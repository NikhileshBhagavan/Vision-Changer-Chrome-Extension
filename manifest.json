{

    "name": "Vision Changer",
    "description": "Toggle According to Time",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": ["content.js"]
    }],

    "permissions": [
        "tabs"
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "images/get_started16.png"
    }
}